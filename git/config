[pull]
	rebase = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[include]
	# Read local config, for user info
	path = config.local
[commit]
	gpgsign = true
[tag]
	gpgsign = true
[gpg]
	format = ssh
[core]
	autocrlf = true
	eol = lf
